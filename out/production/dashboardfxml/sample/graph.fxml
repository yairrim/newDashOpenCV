<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.CategoryAxis?>
<GridPane fx:controller="sample.graphController" xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10" fx:id="grid" stylesheets="@graph.css">
    <LineChart fx:id="drive" title="drive rpm" GridPane.columnIndex="0" GridPane.rowIndex="0" stylesheets="@graph.css">
        <xAxis>
            <CategoryAxis fx:id="drivex" side="BOTTOM"/>
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="drivey" side="LEFT"/>
        </yAxis>
    </LineChart>

    <LineChart fx:id="angle" title="angle" GridPane.columnIndex="1" GridPane.rowIndex="0" stylesheets="@graph.css">
        <xAxis>
            <CategoryAxis fx:id="anglex" side="BOTTOM"/>
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="angley" side="LEFT"/>
        </yAxis>
    </LineChart>

    <LineChart fx:id="liftRPM" title="lift rpm" GridPane.columnIndex="1" GridPane.rowIndex="1" stylesheets="@graph.css">
        <xAxis>
            <CategoryAxis fx:id="liftrpmx" side="BOTTOM"/>
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="liftrpmy" side="LEFT"/>
        </yAxis>
    </LineChart>

    <LineChart fx:id="liftticks" title="lift ticks" GridPane.columnIndex="0" GridPane.rowIndex="1" stylesheets="@graph.css">
        <xAxis>
            <CategoryAxis fx:id="liftticksx" side="BOTTOM"/>
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="liftticksy" side="LEFT"/>
        </yAxis>
    </LineChart>

</GridPane>